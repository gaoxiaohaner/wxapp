@font-face {
	    font-family: 'tuiCollapse';
	    src: url(data:application/font-woff;charset=utf-8;base64,) format('woff');
	    font-weight: normal;
	    font-style: normal;
	}

	.tui-collapse-icon {
		font-family: "tuiCollapse" !important;
		font-style: normal;
		-webkit-font-smoothing: antialiased;
		-moz-osx-font-smoothing: grayscale;
	}

	.tui-icon-arrow:before {
		content: "\e600";
	}

	.tui-icon-arrow {
		font-size: 32rpx;
		transform: rotate(0);
		transform-origin: center center;
		transition: all 0.3s;
		position: absolute;
		top: 50%;
		margin-top: -8px;
		right: 30rpx;
	}
	.tui-arrow-padding{
		padding-right: 62rpx;
		box-sizing: border-box;
	}
	.tui-icon-active {
		transform: rotate(180deg);
		transform-origin: center center;
	}

	.tui-header {
		position: relative;
	}
	
	.tui-collapse-body {
		overflow: hidden;
		transition: all 0.3s ease-in-out;
	}
	.tui-opacity{
		opacity: 0.6;
	}